clear all

syms x t1 t2 d1 d2 y kn1 kn2 kf1 kf2 x0 t10 t20

eq1 = -kn1*t1*x-kn2*t2*x +kf1*d1 +kf2*d2    == 0;
eq2 = -kn1*t1*x  +kf1*d1 -kn1*t1*d2 +kf1*y  == 0;
eq3 = -kn2*t2*x  +kf2*d2 -kn2*t2*d1 +kf2*y  == 0;
eq4 = +kn1*t1*x  -kf1*d1 -kn2*t2*d1 +kf2*y  == 0;
eq5 = +kn2*t2*x  -kf2*d2 -kn1*t1*d2 +kf1*y  == 0;
eq6 = +kn1*t1*d2 +kn2*t2*d1 -(kf1+kf2)*y    == 0;


cn1 = x+d1+y  ==  x0;
cn2 = t1+d1+y == t10;
cn3 = t2+d2+y == t20;


s1 = solve([eq1,eq2,eq3, eq4, eq5, eq6], [x, t1, t2, d1, d2, y])